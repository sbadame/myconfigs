# pacman aliases
alias install="sudo yaourt -S"
alias uninstall="sudo yaourt -Rs"
alias package-search="yaourt -Ss"
alias upgrade="sudo yaourt -Syu"
alias update-package-list="sudo yaourt -Sy"

#Virtualbox crud
alias load-vbox="sudo modprobe vboxdrv"

#Start mysqld
alias startmysql="sudo rc.d start mysqld"
